{
  "name": "paralless",
  "version": "0.2.0",
  "description": "Simple Parallax powered by CSS3 Transitions",
  "main": "dist/paralless.min.js",
  "jsnext:main": "src/paralless.js",
  "scripts": {
    "build": "mkdir -p dist && browserify src/paralless.js --standalone paralless > dist/paralless.browserify.js"
  },
  "browserify": {
    "debug": true,
    "cache": {},
    "packageCache": {},
    "extension": [
      "js"
    ],
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/the-couch/paralless.git"
  },
  "keywords": [
    "javascript",
    "vanilla",
    "parallax",
    "es6"
  ],
  "author": "iamkevingreen <meetus@thecouch.nyc> (http://thecouch.nyc)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/the-couch/paralless/issues"
  },
  "homepage": "https://github.com/the-couch/paralless#readme",
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "*",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-rollup": "*",
    "babelify": "^7.3.0",
    "browser-sync": "*",
    "browserify": "^13.1.0",
    "del": "*",
    "gulp": "*",
    "mkdirp": "*",
    "node-notifier": "*",
    "rollup": "*",
    "rollup-plugin-babel": "*",
    "rollup-plugin-commonjs": "*",
    "rollup-plugin-node-resolve": "*",
    "rollup-plugin-uglify": "*"
  }
}
